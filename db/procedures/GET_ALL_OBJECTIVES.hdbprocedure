PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ALL_OBJECTIVES"(
	OUT out_result TABLE (OBJECTIVE_ID BIGINT, 
						  NAME NVARCHAR(255)
						)
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
out_result = select 
	OBJECTIVE_ID,
	NAME
 from OBJECTIVE
 where ENABLED = 1 and DELETED = 0 
 order by NAME asc;
END;
