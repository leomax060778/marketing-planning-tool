PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ALL_OBJECTIVES"(
		OUT out_result TABLE (
			OBJECTIVE_ID BIGINT,
			NAME NVARCHAR(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT OBJECTIVE_ID, 
				NAME
			FROM OBJECTIVE
			WHERE ENABLED = 1
				AND DELETED = 0
			ORDER BY NAME ASC;
END;
