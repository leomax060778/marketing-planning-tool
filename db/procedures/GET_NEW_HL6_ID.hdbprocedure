PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_NEW_HL6_ID" (
	IN IN_HL5_ID BIGINT,
    OUT  OUT_RESULT BIGINT
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

	SELECT IFNULL(MAX(CAST(ACRONYM AS BIGINT)),0) + 1 INTO OUT_RESULT FROM HL6
	WHERE HL6.HL5_ID = IN_HL5_ID AND HL6.DELETED = 0;
END;
