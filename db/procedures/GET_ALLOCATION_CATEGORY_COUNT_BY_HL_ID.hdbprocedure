PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ALLOCATION_CATEGORY_COUNT_BY_HL_ID" (
	IN in_hl_id bigint,
	OUT out_result int
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL" AS
BEGIN
	SELECT COUNT(DISTINCT allocation_category_id) into out_result
	FROM allocation_category_option_level
	where hierarchy_level_id = in_hl_id
	AND deleted = 0 and enabled = 1;
END;
