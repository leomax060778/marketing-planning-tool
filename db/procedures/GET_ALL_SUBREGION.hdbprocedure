PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ALL_SUBREGION" (
OUT out_result TABLE(
	SUBREGION_ID BIGINT,
	SUBREGION_NAME nvarchar(255),
	SUBREGION_ISO nvarchar(100),
	REGION_ID BIGINT
))
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
out_result = 
		
	SELECT 
	SUBREGION_ID,
	SUBREGION_NAME,
	SUBREGION_ISO,
	REGION_ID
    FROM SUBREGION
 
	WHERE ENABLED = 1 AND DELETED = 0
	ORDER BY SUBREGION_NAME;
END;
