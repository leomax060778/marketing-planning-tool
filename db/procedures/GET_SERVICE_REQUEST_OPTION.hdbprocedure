PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_SERVICE_REQUEST_OPTION" (
	OUT out_result TABLE (
	service_request_option_id BIGINT
	, name NVARCHAR(60)
 	)
 	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

		out_result = SELECT service_request_option_id,
			   name
		FROM SERVICE_REQUEST_OPTION
		WHERE enabled = 1 AND deleted = 0
		ORDER BY upper(name);
END;
