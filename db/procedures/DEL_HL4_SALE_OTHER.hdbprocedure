PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::DEL_HL4_SALE_OTHER" (
	IN in_hl4_id BIGINT,
	OUT out_result BIGINT) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	AS
BEGIN

		DELETE FROM "PLANNING_TOOL"."HL4_SALE_OTHER"
		WHERE hl4_id = in_hl4_id;
		
		SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;
