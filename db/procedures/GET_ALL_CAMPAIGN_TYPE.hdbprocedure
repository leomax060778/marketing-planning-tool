PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ALL_CAMPAIGN_TYPE" (
	OUT out_result TABLE (CAMPAIGN_TYPE_ID BIGINT, 
						  NAME NVARCHAR(255)
						)
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

out_result = select 
	CAMPAIGN_TYPE_ID,
	NAME
 from CAMPAIGN_TYPE
 where 
 ENABLED = 1 and DELETED = 0
 order by NAME asc;
END;
