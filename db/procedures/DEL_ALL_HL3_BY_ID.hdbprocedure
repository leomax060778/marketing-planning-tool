PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::DEL_ALL_HL3_BY_ID" (
	IN in_hl3_id BIGINT,
	OUT out_result BIGINT) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	AS
BEGIN

		DELETE FROM HL3_USER
		WHERE hl3_id = in_hl3_id;
		SELECT ::ROWCOUNT INTO out_result FROM DUMMY;

END;
