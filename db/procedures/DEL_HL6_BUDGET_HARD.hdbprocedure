PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::DEL_HL6_BUDGET_HARD"
(
	in in_hl6_id bigint,
	out out_result bigint
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL" AS
BEGIN

	DELETE FROM HL6_BUDGET
    where HL6_ID = in_hl6_id;
	
	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;