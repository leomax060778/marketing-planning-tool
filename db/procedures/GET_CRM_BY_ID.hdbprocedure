PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_CRM_BY_ID" ( 
	IN in_crm_id BIGINT,
	OUT out_result TABLE (crm_id BIGINT, crm_long_hl3_id NVARCHAR(25), description NVARCHAR (127), h1_id BIGINT, origin_plan_id BIGINT)
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

	out_result =
	
	SELECT crm_id, crm_long_hl3_id, description, h1_id, origin_plan_id
	FROM CRM
	WHERE crm_id = in_crm_id
	  AND deleted = 0
	  AND enabled = 1;

END;