PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::DEL_ALL_HL1_BY_ID" (
	IN in_hl1_id BIGINT,
	OUT out_result BIGINT)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	AS
BEGIN

		DELETE FROM HL1_USER
		WHERE hl1_id = in_hl1_id;
		SELECT ::ROWCOUNT INTO out_result FROM DUMMY;

END;
