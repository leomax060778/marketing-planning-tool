PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ALL_SALES_ORGANIZATIONS" (
	OUT out_result TABLE (SALES_ORGANIZATION_ID BIGINT, 
						  NAME NVARCHAR(255)
						)
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
out_result = 
select 
	SALES_ORGANIZATION_ID
	,NAME
	from SALE_ORGANIZATION
	where ENABLED = 1 AND DELETED = 0
	ORDER BY NAME ASC;
END;
