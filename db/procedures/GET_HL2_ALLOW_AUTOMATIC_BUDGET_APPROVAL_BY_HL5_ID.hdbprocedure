PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_HL2_ALLOW_AUTOMATIC_BUDGET_APPROVAL_BY_HL5_ID" (
	IN in_hl5_id BIGINT
	,OUT out_result TINYINT
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

	SELECT HL2.ALLOW_AUTOMATIC_BUDGET_APPROVAL INTO out_result
	FROM HL2
	INNER JOIN HL3 ON HL2.HL2_ID = HL3.HL2_ID
	INNER JOIN HL4 ON HL3.HL3_ID = HL4.HL3_ID
	INNER JOIN HL5 ON HL4.HL4_ID = HL5.HL4_ID
	WHERE
		HL5.hl5_id = in_hl5_id;

END;