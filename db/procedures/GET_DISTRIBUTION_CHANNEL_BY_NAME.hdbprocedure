PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_DISTRIBUTION_CHANNEL_BY_NAME" (
		IN IN_NAME NVARCHAR(255),
		OUT out_result TABLE (
		    DISTRIBUTION_CHANNEL_ID BIGINT
		    , NAME NVARCHAR(255)
		    , CRM_KEY NVARCHAR(255)
		    )
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT DISTRIBUTION_CHANNEL_ID
	, NAME
	, CRM_KEY
	FROM DISTRIBUTION_CHANNEL
	WHERE NAME = IN_NAME
	and deleted = 0 and enabled = 1;
END;
