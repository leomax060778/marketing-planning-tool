PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::INS_HL6_ALLOCATION_CATEGORY_OPTION" (
    IN categoryOption TABLE(
                       in_category_option_level_id bigint,
                       in_hl6_id bigint,
                       in_amount DECIMAL(19, 6),
                       in_created_user_id bigint,
                       in_updated TINYINT
               		),
    OUT out_result_id bigint
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL" AS
BEGIN
	INSERT INTO "PLANNING_TOOL"."HL6_CATEGORY_OPTION" (allocation_category_option_level_id, hl6_id, amount, created_user_id, updated)
	(select in_category_option_level_id, in_hl6_id, in_amount, in_created_user_id, in_updated from :categoryOption);
	--VALUES (in_category_option_level_id,in_id,in_amount,in_created_user_id, in_updated);
	
	SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO out_result_id FROM "PLANNING_TOOL"."HL6_CATEGORY_OPTION";
END;
