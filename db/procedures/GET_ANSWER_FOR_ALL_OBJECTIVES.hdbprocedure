PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ANSWER_FOR_ALL_OBJECTIVES" (
	OUT out_result TABLE (
	OBJECTIVE_ID BIGINT
	, ANSWER NVARCHAR(1000)
	)
 	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

		out_result = SELECT OBJECTIVE_ID , ANSWER
		FROM ANSWER_TO_OBJECTIVES
		ORDER BY ANSWER;

END;
