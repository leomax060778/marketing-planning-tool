PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_SALES_ORGANIZATIONS_BY_NAME" (
    IN in_name NVARCHAR(255),
	OUT out_result TABLE (SALES_ORGANIZATION_ID BIGINT, 
						  NAME NVARCHAR(255)
						)
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
out_result = 
select 
	SALES_ORGANIZATION_ID
	,NAME
	from SALE_ORGANIZATION
	where ENABLED = 1 AND DELETED = 0 AND UPPER(NAME) = UPPER(in_name)
	ORDER BY NAME ASC;
END;
