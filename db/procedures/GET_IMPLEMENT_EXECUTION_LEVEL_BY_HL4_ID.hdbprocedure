PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_IMPLEMENT_EXECUTION_LEVEL_BY_HL4_ID" (
    IN in_hl4_id BIGINT
	, OUT out_result TINYINT
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

	SELECT 
		HL2.IMPLEMENT_EXECUTION_LEVEL INTO out_result
	FROM HL2
	INNER JOIN HL3 ON HL3.HL2_ID = HL2.HL2_ID
	INNER JOIN HL4 ON HL4.HL3_ID = HL3.HL3_ID
	WHERE HL4.HL4_ID = in_hl4_id;

END;
