PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_CURRENCY_VALUE_BY_ID" (
	IN in_euro_conversion_id BIGINT,
	OUT out_currency_value DECIMAL(19, 6)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	SELECT currency_value INTO out_currency_value
	FROM CURRENCY
	WHERE EURO_CONVERSION_ID = in_euro_conversion_id;
END;
