PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_CAMPAIGN_SUB_TYPE_BY_ID"(
		IN in_campaign_sub_type_id BIGINT, 
		OUT out_result TABLE (
			CAMPAIGN_SUB_TYPE_ID BIGINT,
			NAME NVARCHAR(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT CAMPAIGN_SUB_TYPE_ID, 
				NAME
			FROM CAMPAIGN_SUB_TYPE
			WHERE CAMPAIGN_SUB_TYPE_ID = in_campaign_sub_type_id
				AND ENABLED = 1
				AND DELETED = 0;
END;
