PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_PRIORITY_BY_ID" (
		IN in_priority_id BIGINT,
		OUT out_result TABLE (PRIORITY_ID INTEGER, NAME NVARCHAR(255))
	 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT PRIORITY_ID
						, NAME
				FROM PRIORITY
				WHERE PRIORITY.PRIORITY_ID = in_priority_id;
END;
