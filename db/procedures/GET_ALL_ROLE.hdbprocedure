PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ALL_ROLE" ( 
OUT out_result TABLE(ROLE_ID bigint, NAME nvarchar(25)))
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
/***************************** 
	Write your procedure logic 
 *****************************/
 out_result = SELECT ROLE_ID, NAME
	FROM "ROLE"
 	WHERE ENABLED = 1 AND
 	DELETED = 0;
END;
