PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_EXPECTED_OUTCOME_BY_ID" (
		IN in_expected_outcome_id bigint,
		OUT out_result TABLE(
			expected_outcome_id bigint,
			name nvarchar(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	out_result =
	SELECT expected_outcome_id, name
	from EXPECTED_OUTCOME
    where deleted = 0 and enabled = 1 and expected_outcome_id = in_expected_outcome_id;
END;
