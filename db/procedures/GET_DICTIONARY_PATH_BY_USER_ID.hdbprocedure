PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_DICTIONARY_PATH_BY_USER_ID" ( 
	IN IN_USER_ID BIGINT,
	OUT OUT_RESULT TABLE (
		PATH NVARCHAR(255),
		HIERARCHY_LEVEL_ID TINYINT
		)
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	
	OUT_RESULT =	
	SELECT DISTINCT	 PATH , HIERARCHY_LEVEL_ID
	FROM DICTIONARY_L5_L6   
	WHERE CREATED_USER_ID = IN_USER_ID		
	ORDER BY PATH;
	
END;

