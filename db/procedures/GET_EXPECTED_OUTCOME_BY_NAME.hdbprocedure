PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_EXPECTED_OUTCOME_BY_NAME"
(
        IN IN_NAME NVARCHAR(255),
        OUT out_result TABLE(
              EXPECTED_OUTCOME_ID BIGINT
            , NAME NVARCHAR(255)
        )
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
DEFAULT SCHEMA "PLANNING_TOOL"
AS
BEGIN
	
out_result =
	select 
	 EXPECTED_OUTCOME_ID
	,NAME
	FROM EXPECTED_OUTCOME
	WHERE 
	upper(name) = upper(IN_NAME)
	and deleted = 0 and enabled = 1;		
END;
