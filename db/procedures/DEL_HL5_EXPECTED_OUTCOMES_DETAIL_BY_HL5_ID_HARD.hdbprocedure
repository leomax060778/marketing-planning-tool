PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::DEL_HL5_EXPECTED_OUTCOMES_DETAIL_BY_HL5_ID_HARD" (
	IN in_hl5_id BIGINT,
	OUT out_result BIGINT)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	AS
BEGIN

		DELETE FROM  HL5_EXPECTED_OUTCOMES_DETAIL

	WHERE HL5_EXPECTED_OUTCOMES_ID in (
		select DISTINCT HL5_EXPECTED_OUTCOMES_ID FROM HL5_EXPECTED_OUTCOMES
		WHERE hl5_id = in_hl5_id
	);

	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;

END;
