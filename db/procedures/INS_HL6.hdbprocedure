    ,in in_street nvarchar(255)
    ,in in_postal_code nvarchar(20)
	,OUT out_hl6_id bigint 
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL" AS
BEGIN
	INSERT INTO HL6
	(
 hl6_crm_description
,acronym
,budget
,hl5_id
,route_to_market_id
,campaign_objective_id
,campaign_type_id
,campaign_subtype_id
,marketing_program_id
,marketing_activity_id
,actual_start_date
