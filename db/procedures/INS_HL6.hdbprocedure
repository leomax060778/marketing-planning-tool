,show_on_dg_calendar
,business_owner_id
,employee_responsible_id
,cost_center_id
,in_budget
,budget_spend_q1
,budget_spend_q2
,budget_spend_q3
,budget_spend_q4
,euro_conversion_id
,hl6_status_detail_id
,sales_organization_id
,created_user_id
,distribution_channel_id
,venue 
,city 
,country 
,url
,results_campaign_q1
,results_campaign_q2
,results_campaign_q3
,results_campaign_q4
,planned_start_date
,planned_end_date
,street
,postal_code
	)
