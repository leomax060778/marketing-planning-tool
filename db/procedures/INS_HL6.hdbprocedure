,in_created_user_id
,in_distribution_channel_id
,in_venue
,in_city
,in_country
,in_url
,in_results_campaign_q1
,in_results_campaign_q2
,in_results_campaign_q3
,in_results_campaign_q4
,in_planned_start_date
,in_planned_end_date
,in_street
,in_postal_code
);
	
	SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO out_hl6_id FROM "PLANNING_TOOL"."HL6";
