PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_HL1_QUANTITY_BY_BUDGET_YEAR_ID" (
	IN in_budget_year_id BIGINT,
	OUT out_result BIGINT
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

	SELECT COUNT(HL1_ID) INTO out_result
	FROM HL1
	WHERE BUDGET_YEAR_ID = in_budget_year_id
	and deleted = 0 and enabled = 1;

END;