PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_COST_CENTER_COUNT_BY_NAME" (
    IN in_cost_center_id BIGINT
    ,IN in_name NVARCHAR(255)
    ,OUT out_result INTEGER)
    	LANGUAGE SQLSCRIPT
    	SQL SECURITY INVOKER
    	DEFAULT SCHEMA "PLANNING_TOOL"
    	READS SQL DATA AS
    BEGIN
    	SELECT COUNT(COST_CENTER_ID) INTO out_result
     FROM COST_CENTER
     WHERE ENABLED = 1 AND DELETED = 0 AND NAME = in_name AND COST_CENTER_ID != in_cost_center_id;
    END;
