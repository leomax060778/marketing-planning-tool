PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_EXPECTED_OUTCOME_OPTION_BY_NAME" (
	IN in_name NVARCHAR(255),
	OUT out_result TABLE(
	expected_outcome_option_id bigint
	, name nvarchar(255))
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
  out_result = select expected_outcome_option_id, name
 		from expected_outcome_option
 		where upper(name) = upper(in_name) and
 		enabled = 1 and deleted = 0;
END;
