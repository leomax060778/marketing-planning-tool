PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ALLOCATION_OPTION_BY_ID" (
    IN in_option_id BIGINT,
	OUT out_result TABLE (
	allocation_option_id BIGINT
	, name NVARCHAR(60)
 	)
 	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

		out_result = SELECT allocation_option_id, name FROM ALLOCATION_OPTION
		WHERE ALLOCATION_OPTION_ID = in_option_id AND enabled = 1 AND deleted = 0
		ORDER BY name;

END;
