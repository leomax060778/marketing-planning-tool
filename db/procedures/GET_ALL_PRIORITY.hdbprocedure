PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ALL_PRIORITY" (
		OUT out_result TABLE (PRIORITY_ID INTEGER, NAME NVARCHAR(255))
	 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT PRIORITY_ID
						, NAME
				FROM PRIORITY
				WHERE PRIORITY.ENABLED = 1 AND PRIORITY.DELETED = 0;
END;
