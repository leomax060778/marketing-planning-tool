PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_ALL_EXPECTED_OUTCOME_OPTION" (
	out out_result table (
	expected_outcome_option_id bigint,
    name nvarchar(255)
	))
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
		out_result =
		SELECT expected_outcome_option_id, name
		from EXPECTED_OUTCOME_OPTION 
		where deleted = 0 and enabled = 1
		order by upper(name);
END;