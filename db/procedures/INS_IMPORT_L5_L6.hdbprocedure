PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::INS_IMPORT_L5_L6" (
	IN IN_DESCRIPTION NVARCHAR(255),

	IN in_user_id BIGINT,

	OUT OUT_IMPORT_ID INTEGER
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	AS
BEGIN

    INSERT INTO IMPORT_L5_L6(DESCRIPTION,created_user_id)
    VALUES (IN_DESCRIPTION, in_user_id);

	SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO OUT_IMPORT_ID FROM "PLANNING_TOOL"."IMPORT_L5_L6";
END;
