PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::GET_SALES_ORGANIZATIONS_BY_ID" (
    IN in_id bigint,
	OUT out_result TABLE (SALES_ORGANIZATION_ID BIGINT, 
						  NAME NVARCHAR(255)
						)
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
out_result = 
select 
	SALES_ORGANIZATION_ID
	,NAME
	from SALE_ORGANIZATION
	where ENABLED = 1 AND DELETED = 0 AND SALES_ORGANIZATION_ID = in_id
	ORDER BY NAME ASC;
END;
