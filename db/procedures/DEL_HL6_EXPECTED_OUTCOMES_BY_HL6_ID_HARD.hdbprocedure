PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::DEL_HL6_EXPECTED_OUTCOMES_BY_HL6_ID_HARD" (
	IN in_hl6_id BIGINT,
	OUT out_result BIGINT)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	AS
BEGIN

		delete from  HL6_EXPECTED_OUTCOMES
		WHERE hl6_id = in_hl6_id;

		SELECT ::ROWCOUNT INTO out_result FROM DUMMY;

END;
