PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::INS_ROUTE_TO_MARKET" (
		IN in_name NVARCHAR(255),
		IN in_user_id BIGINT,
		OUT out_route_to_market_id BIGINT
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "PLANNING_TOOL"
	AS
BEGIN
	INSERT INTO ROUTE_TO_MARKET (NAME,created_user_id)
	VALUES (in_name, in_user_id);
	
	SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO out_route_to_market_id FROM "PLANNING_TOOL"."ROUTE_TO_MARKET";
END;
