PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::INS_ROUTE_TO_MARKET" (
	IN in_name NVARCHAR(255),
	IN in_crm_key NVARCHAR(16),
	IN in_user_id BIGINT,
	OUT out_result BIGINT
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	AS
BEGIN
		INSERT INTO "PLANNING_TOOL"."ROUTE_TO_MARKET"(name, crm_key, created_user_id)
		VALUES(in_name, in_crm_key, in_user_id);

		SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO out_result FROM "PLANNING_TOOL"."ROUTE_TO_MARKET";
END;