PROCEDURE "PLANNING_TOOL"."xsplanningtool.db.procedures::INS_EXPECTED_OUTCOME" (
		IN in_name NVARCHAR(255),
		IN in_user_id BIGINT,
		OUT out_outcome_id BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "PLANNING_TOOL"
	AS
BEGIN
		INSERT INTO "PLANNING_TOOL"."EXPECTED_OUTCOME" (name,created_user_id)
		VALUES (in_name,in_user_id);

		SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO out_outcome_id FROM "PLANNING_TOOL"."EXPECTED_OUTCOME";
END;