        <viewAttribute datatype="BIGINT" id="TEAM_TYPE_ID"/>
        <viewAttribute datatype="NVARCHAR" id="BUDGET_YEAR" length="255"/>
      </viewAttributes>
      <calculatedViewAttributes/>
      <definition> 
 /********* Begin Procedure Script ************/ 

 BEGIN 
 	 var_out = 

SELECT 
--HL4 INFO
 DISTINCT AT_HL4.HL4_ID AS HL4_ID 
, AT_HL4.HL3_ID AS HL3_ID
, AT_HL4.ACRONYM AS ACRONYM
, AT_HL4.HL4_CRM_DESCRIPTION AS CRM_DESCRIPTION
, AT_HL4.HL4_DETAILS AS INITIATIVE_CAMPAIGN_DETAILS
, AT_HL4.HL4_BUSINESS_DETAILS AS BUSINESS_VALUE
, HL4SD.DETAIL AS STATUS
, AT_HL4_FNC.HL4_FNC_BUDGET_TOTAL_MKT AS BUDGET_K_EUR
, CONCAT(U.FIRST_NAME, CONCAT(' ', U.LAST_NAME)) AS CREATED_BY
, U.USER_ID
-- PLAN INFO
, AT_HL2.HL2_ID
,AT_HL2.ACRONYM AS PLAN
,AT_HL2.DESCRIPTION AS PLAN_DESCRIPTION
,AT_HL2.BUDGET_YEAR_ID
-- REGION INFO
,AT_HL2.REGION_ID
,AT_REGION.REGION_NAME
-- SUBREGION INFO
,AT_HL2.SUBREGION_ID
,AT_SUBREGION.SUBREGION_NAME
--,AT_HL2.CREATED_USER_ID
--CENTRAL TEAM INFO
,AT_HL2.ORGANIZATION_ACRONYM
,AT_HL2.ORGANIZATION_NAME
,AT_HL2.TEAM_TYPE_ID
,AT_BUDGET_YEAR.BUDGET_YEAR
, SUM(AT_HL4_FNC.HL4_FNC_BUDGET_TOTAL_MKT) AS BUDGET_IVESTMENT
FROM  
&quot;_SYS_BIC&quot;.&quot;xsplanningtool.db.data.views/AT_HL4&quot; AT_HL4  
LEFT JOIN &quot;_SYS_BIC&quot;.&quot;xsplanningtool.db.data.views/AT_HL4_STATUS_DETAIL&quot; HL4SD  ON HL4SD.HL4_STATUS_DETAIL_ID = AT_HL4.HL4_STATUS_DETAIL_ID --ESTADO DEL L4 
LEFT JOIN &quot;_SYS_BIC&quot;.&quot;xsplanningtool.db.data.views/AT_HL4_FNC&quot; AT_HL4_FNC ON AT_HL4_FNC.HL4_ID = AT_HL4.HL4_ID -- BUDGET (K EUR) 
LEFT JOIN &quot;_SYS_BIC&quot;.&quot;xsplanningtool.db.data.views/AT_USER&quot; U ON AT_HL4.CREATED_USER_ID = U.USER_ID --USER 
LEFT JOIN &quot;_SYS_BIC&quot;.&quot;xsplanningtool.db.data.views/AT_HL3&quot; AT_HL3 ON AT_HL3.HL3_ID = AT_HL4.HL3_ID 
LEFT JOIN &quot;_SYS_BIC&quot;.&quot;xsplanningtool.db.data.views/AT_HL2&quot; AT_HL2 ON AT_HL2.HL2_ID = AT_HL3.HL2_ID 
	AND AT_HL2.ENABLED = 1 AND AT_HL2.DELETED = 0
LEFT JOIN &quot;_SYS_BIC&quot;.&quot;xsplanningtool.db.data.views/AT_REGION&quot; AT_REGION ON AT_HL2.REGION_ID = AT_REGION.REGION_ID
LEFT JOIN &quot;_SYS_BIC&quot;.&quot;xsplanningtool.db.data.views/AT_SUBREGION&quot; AT_SUBREGION ON AT_HL2.SUBREGION_ID = AT_SUBREGION.SUBREGION_ID
LEFT JOIN &quot;_SYS_BIC&quot;.&quot;xsplanningtool.db.data.views/AT_BUDGET_YEAR&quot; AT_BUDGET_YEAR ON AT_BUDGET_YEAR.BUDGET_YEAR_ID = AT_HL2.BUDGET_YEAR_ID
	
ORDER BY AT_HL4.HL4_ID;

END /********* End Procedure Script ************/</definition>
    </calculationView>
  </calculationViews>
