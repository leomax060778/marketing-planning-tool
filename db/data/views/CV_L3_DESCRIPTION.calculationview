        <descriptions defaultDescription="BUDGET_K_EUR"/>
        <keyMapping columnObjectName="Script_View" columnName="BUDGET_K_EUR"/>
      </attribute>
      <attribute id="CREATED_BY" order="9">
        <descriptions defaultDescription="CREATED_BY"/>
        <keyMapping columnObjectName="Script_View" columnName="CREATED_BY"/>
      </attribute>
      <attribute id="USER_ID" order="10">
        <descriptions defaultDescription="USER_ID"/>
        <keyMapping columnObjectName="Script_View" columnName="USER_ID"/>
      </attribute>
    </attributes>
    <calculatedAttributes/>
